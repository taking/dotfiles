#!/usr/bin/env bash

# 소스 파일 경로
SourceFile="$HOME/.dotfiles/config/vscode-settings.json"
# VS Code 설정 파일 경로
VSCodeDestination="${HOME}/Library/Application Support/Code/User/settings.json"
# Cursor 설정 파일 경로
CursorDestination="${HOME}/Library/Application Support/Cursor/User/settings.json"

rm -f ${VSCodeDestination}
rm -f ${CursorDestination}
ln -s $HOME/.dotfiles/config/vscode-settings.json ${VSCodeDestination}
ln -s $HOME/.dotfiles/config/vscode-settings.json ${CursorDestination}